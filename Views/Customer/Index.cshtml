@model IEnumerable<MVCAppDemoWithAuthtentication.Models.Customer>
@{
    ViewBag.Title = "Index";
}

<h2>Customer List</h2>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>Customer Name</th>
            <th>Discount Rate</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var customers in Model)
        {
            <tr>
                <td>@Html.ActionLink(customers.Name, "Details", "Customers", new { id = customers.Id }, null)</td>
                <td>@customers.memberShipType.DiscountRate%</td>


               

            </tr>
        }
      
    }

        </tbody>





    </table>

